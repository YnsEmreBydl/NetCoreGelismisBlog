@using EntityLayer.Concrete
@model List<Category>
@{
    Layout = null;
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Kategori Adı</th>
        <th>Durum</th>
        <th>Detay</th>
        <th>Sil</th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>@item.CategoryID</td>
    <td>@item.CategoryName</td>
    <td>@item.CategoryStatus</td>
    <td><a href="#" class="btn btn-warning btn-sm">Detay</a></td>
    <td><a href="#" class="btn btn-danger btn-sm">Sil</a></td>

</tr>

    }
</table>
